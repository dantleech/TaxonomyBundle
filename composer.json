{
    "name": "symfony-cmf/tag-bundle",
    "description": "Bundle which handles automatic persistance of tags for documents stored in PHPCR-ODM",
    "authors": [
        {
            "name": "dantleech",
            "email": "daniel@dantleech.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/framework-bundle": ">=2.1.7,<2.3-dev",
        "jms/metadata": "1.3.0"
    },
    "require-dev": {
        "symfony/class-loader": ">=2.1,<2.3-dev",
        "doctrine/doctrine-bundle": "1.*",
        "doctrine/phpcr-odm": "1.0.*",
        "doctrine/phpcr-bundle": "1.0.*",
        "symfony/yaml": ">=2.1,<2.3-dev",
        "symfony/form": ">=2.1,<2.3-dev",
        "symfony/finder": ">=2.1,<2.3-dev",
        "doctrine/phpcr-odm": "1.0.*",
        "doctrine/phpcr-bundle": "1.0.*",
        "jackalope/jackalope-doctrine-dbal": "1.0.*",
        "doctrine/common": "2.3.x-dev"
    },
    "suggest": {
        "doctrine/phpcr-bundle": "To enable support for the PHPCR ODM documents"
    },
    "autoload": {
        "psr-0": { "Symfony\\Cmf\\Bundle\\TagBundle": "" }
    },
    "target-dir": "Symfony/Cmf/Bundle/TagBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
